<div class="banner">
  <h1>Create a new list</h1>
</div>

<div class="container">
  <div class="row mt-3 mb-3">
    <div class="col-sm-12 col-md-6 col-lg-4">
      <%= simple_form_for @list do |f| %>
      <%= f.input :name %>
      <%= f.input :photos, as: :file %>
      <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
